Private Sub Frame3D2_DblClick(Index As Integer)
   If Index = 2 Then
      If Frame3D2(Index).BorderType = [Panel Inserted] Then
         Frame3D2(Index).BorderType = [Panel Raised]
         Frame3D2(Index).BevelInner = [None Bevel]
         Frame3D2(Index).BackColor = vbButtonFace
      Else
         Frame3D2(Index).BorderType = [Panel Inserted]
         Frame3D2(Index).BackColor = &HF8DECB
         Frame3D2(Index).BevelInner = [Inserted Bevel]
      End If
   End If

End Sub

Private Sub Frame3D5_Click()
   MessageBeep MB_IconQuestion
End Sub

