Private Sub Form_Initialize()
    
   '/* Link XP style controls
   Call ManifestWrite
    
   '/* Multiline text
   Frame3D12.Caption = "Border Type = rPanel Inserted" & vbNewLine & "BevelInner = Inserted"
    
