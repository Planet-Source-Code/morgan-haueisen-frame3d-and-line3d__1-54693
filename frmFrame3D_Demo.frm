   Line3D1.left = Frame3D6.InsideLeft
   Line3D1.Width = Frame3D6.InsideWidth
   Line3D1.top = (Frame3D6.InsideHeight \ 2) + 1
      
   Line3D2.top = Frame3D7(2).InsideTop
   Line3D2.Height = Frame3D7(2).InsideHeight
   Line3D2.left = Frame3D7(2).Width / 2
   
   Frame3D5.Caption = "Click to" & vbNewLine & "Beep"
   Frame3D2(2).Caption = "Double" & vbNewLine & "Click"
   
End Sub

Private Sub Form_Unload(Cancel As Integer)
    
   Call EndApp
   Set frmDemo = Nothing
